<script>


	var mymap = L.map('mapid').setView([40.8206, -96.7056], 17);
	L.tileLayer('http://tile.stamen.com/toner/{z}/{x}/{y}.png', {
		maxZoom: 19,
		attribution: '>Map tiles by <a href="http://stamen.com">Stamen Design</a>, under <a href="http://creativecommons.org/licenses/by/3.0">CC BY 3.0</a>. Data by <a href="http://openstreetmap.org">OpenStreetMap</a>, under<a href="http://www.openstreetmap.org/copyright">ODbL</a>.',
		subdomains: 'abcd'
	}).addTo(mymap);
	L.marker([40.8206, -96.7056]).addTo(mymap)
		.bindPopup("<b>Memorial Stadium</b>");


	L.circle([40.8207, -96.7056], 140, {
		color: 'white',
		fillColor: '#DF1032',
		fillOpacity: 0.5
	}).addTo(mymap);


	L.polygon([[40.819608, -96.703773],[40.819801, -96.703783],[40.819777, -96.703305],[40.819601, -96.703306]], {
			color: 'white',
			fillColor: '#24AF60',
			fillOpacity: 0.5
		}).addTo(mymap);
	L.marker([40.819666, -96.703550]).addTo(mymap)
			.bindPopup("<b>Ye ol' Old Father</b>");

		var popup = L.popup();
		function onMapClick(e) {
			popup
				.setLatLng(e.latlng)
				.setContent("Coordinates" + e.latlng.toString())
				.openOn(mymap);
	};

	L.marker([40.817985, -96.70254]).addTo(mymap).addTo(mymap)
			.bindPopup("<b>Books n junk</b>");

		var geojsonMarkerOptions = {
			radius: 6,
	 	 fillColor: "#ff7800",
	 	 color: "#000",
	 	 weight: 1,
	 	 opacity: 1,
	 	 fillOpacity: 0.6
	  };

		var geojsonFeature = {
				"type": "Feature",
				"properties": {
					"name": "Love Library",
					"amenity:": "Library"
				},
				"geometry": {
						"type": "Polygon",
						"coordinates": ([-96.70254, 40.817985], [-96.70254, 40.82], [-96.8254, 40.83])
				}
		};
		L.geoJSON(geojsonFeature, {
		pointToLayer: function (feature, latlng) {
			return L.marker(latlng);
		}
	}).addTo(mymap);
//function onEachFeature(feature, layer) {
//if (feature.properties && feature.properties.popupContent) {
